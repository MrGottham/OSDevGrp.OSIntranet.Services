<?xml version="1.0"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <configSections>
    <section name="iocConfiguration" type="OSDevGrp.OSIntranet.CommonLibrary.IoC.ConfigurationSectionHandler, OSDevGrp.OSIntranet.CommonLibrary.IoC"/>
  </configSections>

  <iocConfiguration>
    <containerConfigurationProviders>
      <add name="externalCommunication" type="OSDevGrp.OSIntranet.Infrastructure.IoC.ExternalCommunicationConfigurationProvider, OSDevGrp.OSIntranet.Infrastructure" xdt:Transform="SetAttributes(type)" xdt:Locator="Match(name)"/>
    </containerConfigurationProviders>
  </iocConfiguration>

  <system.serviceModel>
    <bindings>
      <ws2007FederationHttpBinding>
        <binding name="OSDevGrp.OSIntranet.SecureByCertificateTransactionFederationHttpBinding">
          <security>
            <message>
              <issuer address="http://localhost/osintranet/basicsecuritytokenservice.svc/WSX509" bindingConfiguration="OSDevGrp.OSIntranet.SecureByCertificateTransactionHttpBinding" xdt:Transform="SetAttributes(address)" xdt:Locator="Match(bindingConfiguration)">
              </issuer>
              <issuerMetadata address="http://localhost/osintranet/basicsecuritytokenservice.svc/mex" xdt:Transform="SetAttributes(address)">
              </issuerMetadata>
            </message>
          </security>
        </binding>
        <binding name="OSDevGrp.OSIntranet.SecureByUserNameTransactionFederationHttpBinding">
          <security>
            <message>
              <issuer address="http://localhost/osintranet/basicsecuritytokenservice.svc/WSUserName" bindingConfiguration="OSDevGrp.OSIntranet.SecureByUserNameTransactionHttpBinding" xdt:Transform="SetAttributes(address)" xdt:Locator="Match(bindingConfiguration)">
              </issuer>
              <issuerMetadata address="http://localhost/osintranet/basicsecuritytokenservice.svc/mex" xdt:Transform="SetAttributes(address)">
              </issuerMetadata>
            </message>
          </security>
        </binding>
      </ws2007FederationHttpBinding>
    </bindings>
    <behaviors>
      <serviceBehaviors>
        <behavior name="OSDevGrp.OSIntranet.IoCServiceBehavior">
          <serviceCredentials>
            <issuedTokenAuthentication>
              <allowedAudienceUris xdt:Transform="Replace">
                <add allowedAudienceUri="http://localhost/osintranet/adressekartotekservice.svc"/>
                <add allowedAudienceUri="http://localhost/osintranet/finansstyringservice.svc"/>
                <add allowedAudienceUri="http://localhost/osintranet/kalenderservice.svc"/>
                <add allowedAudienceUri="http://localhost/osintranet/commonservice.svc"/>
                <add allowedAudienceUri="http://localhost/osintranet/foodwastesystemdataservice.svc"/>
                <add allowedAudienceUri="http://localhost/osintranet/foodwastehouseholddataservice.svc"/>
              </allowedAudienceUris>
            </issuedTokenAuthentication>
          </serviceCredentials>
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>