    <add name="OSDevGrp.OSIntranet.Repositories.DataProviders.MySqlDataProvider" connectionString="SERVER=lxweb; DATABASE=oswebdb; UID=ODBC"/>
    <add name="OSDevGrp.OSIntranet.Repositories.DataProviders.FoodWasteDataProvider" connectionString="SERVER=lxweb; DATABASE=foodwaste; UID=ODBC"/>
  </connectionStrings>
  
  <appSettings>
    <add key="DebitorSaldoOverNul" value="False"/>
    <add key="KreditorSaldoOverNul" value="True"/>
    <add key="DageForBogføringsperiode" value="30"/>
  </appSettings>
  
  <system.web>
    <httpRuntime executionTimeout="600"/>
    <compilation targetFramework="4.5"/>
    <pages controlRenderingCompatibilityVersion="4.0"/>
  </system.web>
  
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="OSDevGrp.OSIntranet.BasicHttpBinding" maxReceivedMessageSize="4194304" sendTimeout="00:05:00" receiveTimeout="00:05:00">
          <security mode="None"/>
        </binding>
      </basicHttpBinding>
      <wsHttpBinding>
        <binding name="OSDevGrp.OSIntranet.SecureByCertificateTransactionHttpBinding" transactionFlow="true" maxReceivedMessageSize="4194304" sendTimeout="00:05:00" receiveTimeout="00:05:00">
